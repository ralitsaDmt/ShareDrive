@model IEnumerable<ShareDrive.ViewModels.Admin.Car.IndexViewModel>

@Html.Partial("_NotificationSuccessPartial")
@Html.Partial("_NotificationErrorPartial")

<table class="responstable">
    <tr>
        <th>Id</th>
        <th>Brand</th>
        <th>Model</th>
        <th>Year</th>
        <th></th>
    </tr>

    @foreach (var item in @Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Brand</td>
            <td>@item.CarModel</td>
            <td>@item.Year</td>
            <td>
                <a asp-area="Admin" asp-action="Details" asp-controller="Cars" asp-route-id="@item.Id"
                   data-target="#modal-container" tabindex="-1"
                   data-toggle="modal" class="btn btn-info">View</a>
            </td>
        </tr>
    }
</table>